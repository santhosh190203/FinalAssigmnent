@model IEnumerable<BoutiqueShop.DressClass>

@{
    ViewBag.Title = "GetDresses";
}

<h2>GetDresses</h2>

<p>
    @Html.ActionLink("Create New", "AddNewDress")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DressId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DressName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DressType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dressprice)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DressId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DressName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DressType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dressprice)
        </td>
        <td>
            @Html.ActionLink("Add", "AddNewDress", new {  id=item.DressId }) |
            @Html.ActionLink("Details", "Details", new {  id=item.DressId  }) |
            @Html.ActionLink("Delete", "DeleteDress", new {  id=item.DressId })
        </td>
    </tr>
}

</table>
